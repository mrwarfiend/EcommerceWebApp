@model EcommerceProject.Models.Cart
@*
    For more information on enabling MVC for empty projects, visit https://go.microsoft.com/fwlink/?LinkID=397860
*@
@{
    Layout = null;
}
<tr>
    <td>
        @Html.DisplayFor(item => item.userId)
        @Html.HiddenFor(e=>e.userId)
    </td>
    <td>
        @Html.DisplayFor(item => item.prod.name)
        @Html.HiddenFor(e=>e.prod.name)
    </td>
    <td>
        @Html.DisplayFor(item => item.count)
        @Html.HiddenFor(e=>e.count)
    </td>
    <td>
        @Html.DisplayFor(item => item.subtotal)
        @Html.HiddenFor(e=>e.subtotal)
    </td>
    <td>
        @Html.DisplayFor(item => item.tax)
        @Html.HiddenFor(e=>e.tax)
    </td>
    <td>
        @Html.DisplayFor(item => item.total)
        @Html.HiddenFor(e=>e.total)
    </td>
    <td>
        @Html.DisplayFor(item => item.timeStamp)
        @Html.HiddenFor(e=>e.timeStamp)
    </td>
    <td>
        @Html.HiddenFor(e=>e.id)
        @Html.HiddenFor(e=>e.prod)
        @Html.HiddenFor(e=>e.prod.id)
        @Html.CheckBoxFor(e=>e.check)
    </td>
</tr>